{
  "name": "e-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "backend": "json-server --watch db.json --port 5000 --delay=500",
    "backendWithAuth": "json-server --watch db.json --port 5000 --delay=500 -m ./node_modules/json-server-auth",
    "start:fullstack": "concurrently \"yarn run backendWithAuth\" \"yarn run start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.0",
    "@types/react-autosuggest": "10.0.0",
    "concurrently": "5.3.0",
    "json-server": "0.16.1",
    "json-server-auth": "2.0.2",
    "prettier": "2.1.1"
  },
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/pickers": "3.2.10",
    "@reduxjs/toolkit": "1.4.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.2",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "26.0.13",
    "@types/node": "14.6.4",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-test-renderer": "16.9.3",
    "@types/testing-library__jest-dom": "5.9.2",
    "@types/uuid": "8.3.0",
    "@types/webpack-env": "1.15.2",
    "@types/yup": "0.29.6",
    "axios": "0.20.0",
    "bootstrap-utilities": "4.1.3",
    "clsx": "1.1.1",
    "cross-env": "7.0.2",
    "date-fns": "2.16.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "fontfaceobserver": "2.1.0",
    "formik": "2.1.5",
    "i18next": "19.7.0",
    "i18next-browser-languagedetector": "6.0.1",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "10.3.0",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "react": "17.0.0-rc.0",
    "react-app-polyfill": "1.0.6",
    "react-autosuggest": "10.0.2",
    "react-dom": "17.0.0-rc.0",
    "react-helmet-async": "1.0.6",
    "react-i18next": "11.7.2",
    "react-redux": "7.2.1",
    "react-router": "6.0.0-alpha.5",
    "react-router-dom": "6.0.0-alpha.5",
    "react-scripts": "3.4.3",
    "react-select-country-list": "2.2.1",
    "react-swipeable-views": "0.13.9",
    "react-test-renderer": "16.13.1",
    "redux-injectors": "1.3.0",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "replace-in-file": "6.1.0",
    "reselect": "4.0.0",
    "serve": "11.3.2",
    "shelljs": "0.8.4",
    "ts-node": "9.0.0",
    "typesafe-actions": "5.1.0",
    "typescript": "4.0.2",
    "typestyle": "2.1.0",
    "uuid": "8.3.0",
    "yup": "0.29.3"
  }
}
